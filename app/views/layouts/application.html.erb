<!DOCTYPE html>
<html>
  <head>
    <title>Homecoming</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div>
       <%= render 'layouts/navbar' %>
       <%= render_flash_messages %>
    </div>
    <div>
      <% if flash[:message] %>
          <%= flash[:message]%>

      <% end %>
      <%= @error%>
    </div>
    <%= yield %>
  </body>
</html>